    "cross-env": "^5.0.5",
    "d3": "^4.10.2",
    "express": "^4.15.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "nuxt": "^1.0.0-rc8",
    "sass-loader": "^6.0.6",
    "venn.js": "^0.2.15",
    "vue-multiselect": "^2.0.2",
    "vuex": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
