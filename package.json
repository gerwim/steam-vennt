{
  "name": "steam-vennt",
  "version": "1.0.0",
  "description": "Pick the game night game easily with a venn diagram of your friends' steam games",
  "author": "Tom Kitchin <tkitchin@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
